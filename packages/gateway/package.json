  "license": "MIT",
  "dependencies": {
    "@mini_faas_worker/runtime": "workspace:^",
    "chalk": "^5.0.1",
    "fastify": "^3.29.0"
  },
  "devDependencies": {
    "@mini_faas_worker/common": "workspace:^"
  }
}
