{
  "name": "@mini_faas_worker/gateway",
  "version": "0.0.1",
  "description": "gateway for mini_faas_worker",
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": {
    "mini_faas_worker": "dist/index.js"
  },
  "scripts": {
    "watch": "tsup src/index.ts --format=esm --dts --watch",
