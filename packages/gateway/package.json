{
  "name": "@mini_faas_worker/gateway",
  "version": "0.0.1",
  "description": "gateway for mini_faas_worker",
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": {
    "mini_faas_worker": "dist/index.js"
  },
  "scripts": {
    "dev": "pnpm prisma:generate && pnpm prisma:db:push && tsx watch src/index.ts",
    "build": "rimraf ./dist && pnpm prisma:generate && tsc --build --force --verbose",
    "start": "node --experimental-specifier-resolution=node ./dist",
    "lint": "eslint --cache --ignore-pattern dist .",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "npx prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:db:push": "npx prisma db push"
  },
  "keywords": [
    "gateway",
    "faas",
    "worker",
    "v8"
  ],
  "author": "lilong7676@outlook.com",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.1.1",
    "@fastify/multipart": "^7.3.0",
    "@fastify/static": "^6.8.0",
    "@fastify/websocket": "^7.1.2",
    "@mini_faas_worker/common": "workspace:^",
    "@mini_faas_worker/runtime": "workspace:^",
    "@mini_faas_worker/serverless": "workspace:^",
    "fastify": "^4.9.2",
    "ioredis": "^5.2.3",
    "node-fetch": "^3.2.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mini_faas_worker/types": "workspace:^",
    "@prisma/client": "^4.5.0",
    "@types/node": "^18.7.1",
    "@types/ws": "^8.5.3",
    "devtools-protocol": "^0.0.1092731",
    "prisma": "^4.5.0",
    "tsx": "^3.11.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
