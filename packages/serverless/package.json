{
  "name": "@mini_faas_worker/serverless",
  "version": "0.0.1",
  "description": "serverless for mini_faas_worker",
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": {
    "serverless": "dist/run.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/run.ts",
    "build": "rimraf ./dist && tsc --build --force --verbose",
    "start": "node --experimental-specifier-resolution=node ./dist/run.js",
    "lint": "eslint --cache --ignore-pattern dist .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "gateway",
    "faas",
    "worker",
    "v8"
  ],
  "author": "lilong7676@outlook.com",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.1.1",
    "@mini_faas_worker/common": "workspace:^",
    "@mini_faas_worker/sdk": "workspace:^",
    "fastify": "^4.9.2",
    "ioredis": "^5.2.3",
    "node-fetch": "^3.2.6",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@mini_faas_worker/types": "workspace:^"
  }
}
